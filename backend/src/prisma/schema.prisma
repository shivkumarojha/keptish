generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @unique @default(autoincrement())
  name            String
  email           String
  password        String
  isEmailVerified Boolean  @default(false)
  createdAt       DateTime @default(now())
  updatedAT       DateTime
}